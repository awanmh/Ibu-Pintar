{"ast":null,"code":"export default {\n  name: 'TheNavbar'\n};","map":{"version":3,"names":["name"],"sources":["E:\\ibuhamil\\frontend\\src\\components\\layout\\TheNavbar.vue"],"sourcesContent":["<template>\r\n  <header class=\"navbar animate-fade-in\">\r\n    <div class=\"container\">\r\n      <div class=\"brand\">\r\n        <router-link to=\"/\" class=\"brand-link\">\r\n          <img src=\"@/assets/images/tes.png\" alt=\"Ibu Pintar Logo\" class=\"logo-image\" />\r\n        </router-link>\r\n      </div>\r\n      <nav class=\"nav-links\">\r\n        <router-link to=\"/\" exact-active-class=\"active\">Beranda</router-link>\r\n        <router-link to=\"/articles\" exact-active-class=\"active\">Artikel</router-link>\r\n        <router-link to=\"/layanan\" exact-active-class=\"active\">Layanan</router-link>\r\n        <router-link to=\"/tanya-bidan\" exact-active-class=\"active\">Tanya Bidan</router-link>\r\n        <router-link to=\"/about\" exact-active-class=\"active\">Tentang Kami</router-link>\r\n      </nav>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'TheNavbar',\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* ANIMASI MASUK */\r\n@keyframes fadeInDown {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(-20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n.animate-fade-in {\r\n  animation: fadeInDown 0.5s ease-out;\r\n}\r\n\r\n/* NAVBAR UTAMA */\r\n.navbar {\r\n  background-color: #ffffff;\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.04);\r\n  padding: 15px 0;\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 1000;\r\n  transition: background 0.3s ease;\r\n}\r\n\r\n/* CONTAINER */\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 0 24px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n/* BRAND */\r\n.brand-link {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 10px;\r\n  text-decoration: none;\r\n}\r\n.logo-image {\r\n  height: 45px;\r\n}\r\n.brand-text {\r\n  font-size: 1.3rem;\r\n  font-weight: 700;\r\n  color: #c2185b;\r\n}\r\n\r\n/* NAV LINKS */\r\n.nav-links {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 20px;\r\n}\r\n.nav-links a {\r\n  font-weight: 500;\r\n  position: relative;\r\n  padding: 6px 0;\r\n  color: #333;\r\n  text-decoration: none;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n/* Hover & underline effect */\r\n.nav-links a::after {\r\n  content: '';\r\n  position: absolute;\r\n  left: 0;\r\n  bottom: 0;\r\n  height: 2px;\r\n  width: 0;\r\n  background-color: #c2185b;\r\n  transition: width 0.3s ease-in-out;\r\n}\r\n.nav-links a:hover {\r\n  color: #c2185b;\r\n}\r\n.nav-links a:hover::after {\r\n  width: 100%;\r\n}\r\n\r\n/* ACTIVE LINK */\r\n.nav-links .active {\r\n  color: #c2185b;\r\n  font-weight: 700;\r\n}\r\n.nav-links .active::after {\r\n  width: 100%;\r\n  background-color: #c2185b;\r\n}\r\n\r\n/* RESPONSIVE */\r\n@media (max-width: 768px) {\r\n  .nav-links {\r\n    flex-wrap: wrap;\r\n    justify-content: flex-end;\r\n    gap: 10px;\r\n  }\r\n  .brand-text {\r\n    font-size: 1.1rem;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AAoBA,eAAe;EACbA,IAAI,EAAE;AACR,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}