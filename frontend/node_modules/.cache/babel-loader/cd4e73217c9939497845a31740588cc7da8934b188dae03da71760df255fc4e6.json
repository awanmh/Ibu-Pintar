{"ast":null,"code":"import { computed } from 'vue';\nexport default {\n  __name: 'ArticleCard',\n  props: {\n    article: {\n      type: Object,\n      required: true\n    },\n    isHero: {\n      type: Boolean,\n      default: false\n    },\n    isSide: {\n      type: Boolean,\n      default: false\n    }\n  },\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const props = __props;\n\n    // Mengatur class CSS berdasarkan props\n    const cardClasses = computed(() => ['article-card', {\n      'is-hero': props.isHero\n    }, {\n      'is-side': props.isSide\n    }]);\n    const __returned__ = {\n      props,\n      cardClasses,\n      computed\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","props","__props","cardClasses","isHero","isSide"],"sources":["E:/ibuhamil/frontend/src/components/articles/ArticleCard.vue"],"sourcesContent":["<template>\r\n  <div :class=\"cardClasses\">\r\n    <div v-if=\"article.imageUrl\" class=\"card-image\">\r\n      <img :src=\"article.imageUrl\" :alt=\"article.title\">\r\n    </div>\r\n    <div class=\"card-content\">\r\n      <span v-if=\"!isSide\" class=\"category-tag\">{{ article.category.name }}</span>\r\n      <h3 class=\"card-title\">\r\n        <router-link :to=\"`/articles/${article.id}`\">{{ article.title }}</router-link>  \r\n      </h3>\r\n      <p v-if=\"isHero\" class=\"card-excerpt\">\r\n        {{ article.content.substring(0, 150) }}...\r\n      </p>\r\n      <p class=\"card-meta\">\r\n        Oleh <strong>{{ article.author.name }}</strong>\r\n      </p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { computed } from 'vue';\r\n\r\nconst props = defineProps({\r\n  article: { type: Object, required: true },\r\n  isHero: { type: Boolean, default: false },\r\n  isSide: { type: Boolean, default: false },\r\n});\r\n\r\n// Mengatur class CSS berdasarkan props\r\nconst cardClasses = computed(() => [\r\n  'article-card',\r\n  { 'is-hero': props.isHero },\r\n  { 'is-side': props.isSide },\r\n]);\r\n</script>\r\n\r\n<style scoped>\r\n.article-card {\r\n  border: 1px solid #e2e8f0;\r\n  border-radius: 8px;\r\n  overflow: hidden;\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n.card-image img {\r\n  width: 100%;\r\n  display: block;\r\n  aspect-ratio: 16 / 9;\r\n  object-fit: cover;\r\n  background-color: #f0f0f0;\r\n}\r\n.card-content {\r\n  padding: 15px;\r\n  flex-grow: 1;\r\n}\r\n.card-title {\r\n  font-size: 1.1em;\r\n  margin: 5px 0;\r\n}\r\n.card-title a {\r\n  text-decoration: none;\r\n  color: #333;\r\n}\r\n.card-title a:hover {\r\n  color: #ff69b4;\r\n}\r\n.card-meta {\r\n  font-size: 0.8em;\r\n  color: #718096;\r\n}\r\n.category-tag {\r\n  font-size: 0.75em;\r\n  font-weight: bold;\r\n  color: #ff69b4;\r\n}\r\n\r\n/* Style untuk Hero Card */\r\n.article-card.is-hero .card-title {\r\n  font-size: 2em;\r\n}\r\n.article-card.is-hero .card-content {\r\n  padding: 20px;\r\n}\r\n.article-card.is-hero .card-excerpt {\r\n  margin: 15px 0;\r\n  color: #4a5568;\r\n}\r\n\r\n/* Style untuk Side Card */\r\n.article-card.is-side {\r\n  flex-direction: row;\r\n  align-items: center;\r\n  border: none;\r\n  border-bottom: 1px solid #eee;\r\n  border-radius: 0;\r\n  padding-bottom: 15px;\r\n}\r\n.article-card.is-side .card-image {\r\n  flex: 0 0 100px; /* Lebar gambar 100px, jangan membesar/mengecil */\r\n  margin-right: 15px;\r\n}\r\n.article-card.is-side .card-content {\r\n  padding: 0;\r\n}\r\n.article-card.is-side .card-title {\r\n  font-size: 1em;\r\n}\r\n</style>"],"mappings":"AAqBA,SAASA,QAAQ,QAAQ,KAAK;;;;;;;;;;;;;;;;;;;;;IAE9B,MAAMC,KAAK,GAAGC,OAIZ;;IAEF;IACA,MAAMC,WAAW,GAAGH,QAAQ,CAAC,MAAM,CACjC,cAAc,EACd;MAAE,SAAS,EAAEC,KAAK,CAACG;IAAO,CAAC,EAC3B;MAAE,SAAS,EAAEH,KAAK,CAACI;IAAO,CAAC,CAC5B,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}