{"ast":null,"code":"import TheNavbar from '@/components/layout/TheNavbar.vue';\nimport TheFooter from '@/components/layout/TheFooter.vue';\nexport default {\n  __name: 'App',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const __returned__ = {\n      TheNavbar,\n      TheFooter\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["TheNavbar","TheFooter"],"sources":["E:/ibuhamil/frontend/src/App.vue"],"sourcesContent":["<!-- src/App.vue -->\r\n<template>\r\n  <div id=\"app-wrapper\">\r\n    <!-- Navbar akan selalu tampil di atas -->\r\n    <TheNavbar />\r\n\r\n    <!-- \r\n      <router-view> adalah komponen khusus dari Vue Router.\r\n      Di sinilah semua komponen dari folder /views akan dirender.\r\n      Kita bungkus dengan <transition> agar ada efek fade saat berpindah halaman.\r\n    -->\r\n    <main>\r\n      <router-view v-slot=\"{ Component }\">\r\n        <transition name=\"fade\" mode=\"out-in\">\r\n          <component :is=\"Component\" />\r\n        </transition>\r\n      </router-view>\r\n    </main>\r\n    \r\n    <!-- Footer akan selalu tampil di bawah -->\r\n    <TheFooter />\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport TheNavbar from '@/components/layout/TheNavbar.vue';\r\nimport TheFooter from '@/components/layout/TheFooter.vue';\r\n</script>\r\n\r\n<style>\r\n/* Style di sini TIDAK 'scoped' agar bisa berlaku global.\r\n  Ini tempat yang baik untuk mendefinisikan style dasar dan efek transisi.\r\n*/\r\n\r\n/* Efek transisi antar halaman */\r\n.fade-enter-active,\r\n.fade-leave-active {\r\n  transition: opacity 0.3s ease;\r\n}\r\n\r\n.fade-enter-from,\r\n.fade-leave-to {\r\n  opacity: 0;\r\n}\r\n\r\n/* Style dasar untuk layout */\r\n#app-wrapper {\r\n  display: flex;\r\n  flex-direction: column;\r\n  min-height: 100vh;\r\n}\r\n\r\nmain {\r\n  flex-grow: 1; /* Mendorong footer ke bawah halaman */\r\n}\r\n</style>    "],"mappings":"AAyBA,OAAOA,SAAS,MAAM,mCAAmC;AACzD,OAAOC,SAAS,MAAM,mCAAmC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}