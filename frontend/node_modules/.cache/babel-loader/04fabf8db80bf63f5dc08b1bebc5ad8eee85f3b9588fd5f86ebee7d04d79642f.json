{"ast":null,"code":"import ApiService from '@/services/ApiService';\nimport { useRouter } from 'vue-router';\nexport default {\n  setup() {\n    const router = useRouter();\n    const form = reactive({\n      name: '',\n      imageUrl: '',\n      duration: '',\n      category: '',\n      price: 0,\n      booking_link: ''\n    });\n    const handleSubmit = async () => {\n      try {\n        await ApiService.createTreatment(form);\n        router.push('/admin/treatments');\n      } catch (error) {\n        alert('Gagal menyimpan treatment: ' + error.message);\n      }\n    };\n    return {\n      form,\n      handleSubmit\n    };\n  }\n};","map":{"version":3,"names":["ApiService","useRouter","setup","router","form","reactive","name","imageUrl","duration","category","price","booking_link","handleSubmit","createTreatment","push","error","alert","message"],"sources":["E:\\ibuPintar\\frontend\\src\\views\\admin\\TreatmentForm.vue"],"sourcesContent":["<template>\r\n  <div class=\"p-6 max-w-4xl mx-auto\">\r\n    <h2 class=\"text-2xl font-bold mb-6 text-gray-800\">Tambah Treatment</h2>\r\n\r\n    <form @submit.prevent=\"handleSubmit\" class=\"grid grid-cols-1 md:grid-cols-2 gap-4 bg-white p-6 rounded-lg shadow-md\">\r\n      <div>\r\n        <label class=\"block mb-1 text-gray-700\">Nama Treatment</label>\r\n        <input v-model=\"form.name\" type=\"text\" class=\"input\" placeholder=\"Nama Treatment\" required />\r\n      </div>\r\n\r\n      <div>\r\n        <label class=\"block mb-1 text-gray-700\">URL Gambar</label>\r\n        <input v-model=\"form.imageUrl\" type=\"text\" class=\"input\" placeholder=\"URL Gambar\" />\r\n      </div>\r\n\r\n      <div>\r\n        <label class=\"block mb-1 text-gray-700\">Durasi</label>\r\n        <input v-model=\"form.duration\" type=\"text\" class=\"input\" placeholder=\"Contoh: 60 menit\" />\r\n      </div>\r\n\r\n      <div>\r\n        <label class=\"block mb-1 text-gray-700\">Kategori</label>\r\n        <input v-model=\"form.category\" type=\"text\" class=\"input\" placeholder=\"Contoh: Yoga\" />\r\n      </div>\r\n\r\n      <div class=\"md:col-span-2\">\r\n        <label class=\"block mb-1 text-gray-700\">Harga (Rp)</label>\r\n        <input v-model.number=\"form.price\" type=\"number\" class=\"input\" placeholder=\"Contoh: 150000\" min=\"0\" />\r\n      </div>\r\n\r\n      <div class=\"md:col-span-2\">\r\n        <label class=\"block mb-1 text-gray-700\">Link Booking (Opsional)</label>\r\n        <input v-model=\"form.booking_link\" type=\"text\" class=\"input\" placeholder=\"https://link...\" />\r\n      </div>\r\n\r\n      <div class=\"md:col-span-2 text-right mt-4\">\r\n        <button type=\"submit\" class=\"bg-pink-600 hover:bg-pink-700 text-white px-6 py-2 rounded-lg shadow\">\r\n          Simpan\r\n        </button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport ApiService from '@/services/ApiService';\r\nimport { useRouter } from 'vue-router';\r\n\r\nexport default {\r\n  setup() {\r\n    const router = useRouter();\r\n    const form = reactive({\r\n      name: '',\r\n      imageUrl: '',\r\n      duration: '',\r\n      category: '',\r\n      price: 0,\r\n      booking_link: '',\r\n    });\r\n\r\n    const handleSubmit = async () => {\r\n      try {\r\n        await ApiService.createTreatment(form);\r\n        router.push('/admin/treatments');\r\n      } catch (error) {\r\n        alert('Gagal menyimpan treatment: ' + error.message);\r\n      }\r\n    };\r\n\r\n    return { form, handleSubmit };\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input {\r\n  @apply w-full px-3 py-2 border rounded-md shadow-sm focus:outline-none focus:ring-2 focus:ring-pink-500 focus:border-pink-500;\r\n}\r\n</style>\r\n"],"mappings":"AA6CA,OAAOA,UAAS,MAAO,uBAAuB;AAC9C,SAASC,SAAQ,QAAS,YAAY;AAEtC,eAAe;EACbC,KAAKA,CAAA,EAAG;IACN,MAAMC,MAAK,GAAIF,SAAS,CAAC,CAAC;IAC1B,MAAMG,IAAG,GAAIC,QAAQ,CAAC;MACpBC,IAAI,EAAE,EAAE;MACRC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,QAAQ