{"ast":null,"code":"import { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport ArticleCard from '@/components/articles/ArticleCard.vue';\nexport default {\n  __name: 'ArticlesView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const allArticles = computed(() => store.getters['articles/allArticles']);\n    const status = computed(() => store.state.articles.status);\n    const mainArticle = computed(() => allArticles.value[0] || null);\n    const sideArticles = computed(() => allArticles.value.slice(1, 4));\n    const remainingArticles = computed(() => allArticles.value.slice(4));\n    onMounted(() => {\n      store.dispatch('articles/fetchArticles');\n\n      // Logic untuk memicu animasi saat komponen dimuat\n      const observer = new IntersectionObserver(entries => {\n        entries.forEach(entry => {\n          if (entry.isIntersecting) {\n            entry.target.classList.add('is-visible');\n            observer.unobserve(entry.target);\n          }\n        });\n      }, {\n        threshold: 0.1\n      });\n\n      // Terapkan observer ke semua elemen dengan class 'animate-card'\n      // Kita beri sedikit jeda agar elemennya sudah ada di DOM\n      setTimeout(() => {\n        document.querySelectorAll('.animate-card').forEach(el => {\n          observer.observe(el);\n        });\n      }, 100);\n    });\n    const __returned__ = {\n      store,\n      allArticles,\n      status,\n      mainArticle,\n      sideArticles,\n      remainingArticles,\n      computed,\n      onMounted,\n      get useStore() {\n        return useStore;\n      },\n      ArticleCard\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","onMounted","useStore","ArticleCard","store","allArticles","getters","status","state","articles","mainArticle","value","sideArticles","slice","remainingArticles","dispatch","observer","IntersectionObserver","entries","forEach","entry","isIntersecting","target","classList","add","unobserve","threshold","setTimeout","document","querySelectorAll","el","observe"],"sources":["E:/ibuhamil/frontend/src/views/article/ArticlesView.vue"],"sourcesContent":["<template>\r\n  <div class=\"articles-view\">\r\n    <!-- Header Halaman -->\r\n    <section class=\"page-header\">\r\n      <div class=\"container\">\r\n        <h1>Semua Artikel</h1>\r\n        <p>Temukan informasi dan tips terbaru seputar dunia kehamilan.</p>\r\n      </div>\r\n    </section>\r\n\r\n    <!-- Konten Utama -->\r\n    <main class=\"container\">\r\n      <div v-if=\"status === 'loading'\" class=\"loading\">Memuat artikel...</div>\r\n      <div v-if=\"status === 'error'\" class=\"error\">Gagal memuat artikel. Silakan coba lagi nanti.</div>\r\n\r\n      <div v-if=\"status === 'success' && allArticles.length > 0\">\r\n        <!-- Bagian Atas: Hero & Artikel Pendamping -->\r\n        <div class=\"top-section-grid\">\r\n          <div class=\"hero-article animate-card\">\r\n            <ArticleCard :article=\"mainArticle\" :is-hero=\"true\" />\r\n          </div>\r\n          <div class=\"side-articles\">\r\n            <div \r\n              v-for=\"(article, index) in sideArticles\" \r\n              :key=\"article.id\"\r\n              class=\"animate-card\"\r\n              :style=\"{ 'transition-delay': `${index * 100}ms` }\"\r\n            >\r\n              <ArticleCard \r\n                :article=\"article\"\r\n                :is-side=\"true\"\r\n              />\r\n            </div>\r\n          </div>\r\n        </div>\r\n\r\n        <!-- Bagian Bawah: Grid Artikel Lainnya -->\r\n        <div class=\"more-articles-section\">\r\n          <h2 class=\"section-title animate-card\">Artikel Lainnya</h2>\r\n          <div class=\"more-articles-grid\">\r\n            <div\r\n              v-for=\"(article, index) in remainingArticles\" \r\n              :key=\"article.id\"\r\n              class=\"animate-card\"\r\n              :style=\"{ 'transition-delay': `${index * 100}ms` }\"\r\n            >\r\n              <ArticleCard :article=\"article\" />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </div>\r\n      \r\n      <div v-else-if=\"status === 'success' && allArticles.length === 0\" class=\"empty-state\">\r\n          <p>Belum ada artikel yang dipublikasikan.</p>\r\n      </div>\r\n    </main>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { computed, onMounted } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport ArticleCard from '@/components/articles/ArticleCard.vue';\r\n\r\nconst store = useStore();\r\n\r\nconst allArticles = computed(() => store.getters['articles/allArticles']);\r\nconst status = computed(() => store.state.articles.status);\r\n\r\nconst mainArticle = computed(() => allArticles.value[0] || null);\r\nconst sideArticles = computed(() => allArticles.value.slice(1, 4));\r\nconst remainingArticles = computed(() => allArticles.value.slice(4));\r\n\r\nonMounted(() => {\r\n  store.dispatch('articles/fetchArticles');\r\n\r\n  // Logic untuk memicu animasi saat komponen dimuat\r\n  const observer = new IntersectionObserver((entries) => {\r\n    entries.forEach((entry) => {\r\n      if (entry.isIntersecting) {\r\n        entry.target.classList.add('is-visible');\r\n        observer.unobserve(entry.target);\r\n      }\r\n    });\r\n  }, { threshold: 0.1 });\r\n\r\n  // Terapkan observer ke semua elemen dengan class 'animate-card'\r\n  // Kita beri sedikit jeda agar elemennya sudah ada di DOM\r\n  setTimeout(() => {\r\n    document.querySelectorAll('.animate-card').forEach((el) => {\r\n        observer.observe(el);\r\n    });\r\n  }, 100);\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n/* Animasi Fade In dan Slide Up */\r\n.animate-card {\r\n  opacity: 0;\r\n  transform: translateY(30px);\r\n  transition: opacity 0.6s ease-out, transform 0.6s ease-out;\r\n}\r\n\r\n.animate-card.is-visible {\r\n  opacity: 1;\r\n  transform: translateY(0);\r\n}\r\n\r\n/* General Styles */\r\n.articles-view {\r\n  background-color: #f8f9fa; /* Warna latar belakang lembut */\r\n}\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 0 20px;\r\n}\r\n\r\n/* Page Header */\r\n.page-header {\r\n  padding: 60px 0;\r\n  background: linear-gradient(135deg, #fff0f5 0%, #e6e6fa 100%);\r\n  text-align: center;\r\n  margin-bottom: 50px;\r\n}\r\n.page-header h1 {\r\n  font-size: 3em;\r\n  color: #4a4a4a;\r\n}\r\n.page-header p {\r\n  font-size: 1.2em;\r\n  color: #6c757d;\r\n  margin-top: 10px;\r\n}\r\n\r\n/* Loading & Error States */\r\n.loading, .error, .empty-state {\r\n  text-align: center;\r\n  padding: 50px;\r\n  font-size: 1.2em;\r\n  color: #888;\r\n}\r\n\r\n/* Top Section Layout */\r\n.top-section-grid {\r\n  display: grid;\r\n  grid-template-columns: 2fr 1fr;\r\n  gap: 40px;\r\n  margin-bottom: 60px;\r\n}\r\n.side-articles {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 25px;\r\n}\r\n\r\n/* More Articles Section */\r\n.more-articles-section {\r\n  margin-top: 40px;\r\n  padding-top: 40px;\r\n  border-top: 1px solid #e9ecef;\r\n}\r\n.section-title {\r\n  font-size: 2em;\r\n  color: #343a40;\r\n  margin-bottom: 30px;\r\n}\r\n.more-articles-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\r\n  gap: 30px;\r\n}\r\n</style>\r\n"],"mappings":"AA4DA,SAASA,QAAQ,EAAEC,SAAS,QAAQ,KAAK;AACzC,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,WAAW,MAAM,uCAAuC;;;;;;;IAE/D,MAAMC,KAAK,GAAGF,QAAQ,CAAC,CAAC;IAExB,MAAMG,WAAW,GAAGL,QAAQ,CAAC,MAAMI,KAAK,CAACE,OAAO,CAAC,sBAAsB,CAAC,CAAC;IACzE,MAAMC,MAAM,GAAGP,QAAQ,CAAC,MAAMI,KAAK,CAACI,KAAK,CAACC,QAAQ,CAACF,MAAM,CAAC;IAE1D,MAAMG,WAAW,GAAGV,QAAQ,CAAC,MAAMK,WAAW,CAACM,KAAK,CAAC,CAAC,CAAC,IAAI,IAAI,CAAC;IAChE,MAAMC,YAAY,GAAGZ,QAAQ,CAAC,MAAMK,WAAW,CAACM,KAAK,CAACE,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;IAClE,MAAMC,iBAAiB,GAAGd,QAAQ,CAAC,MAAM