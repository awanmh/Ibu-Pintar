{"ast":null,"code":"export default {\n  name: 'TheNavbar'\n};","map":{"version":3,"names":["name"],"sources":["E:\\ibuhamil\\frontend\\src\\components\\layout\\TheNavbar.vue"],"sourcesContent":["<template>\r\n  <header class=\"navbar animate-fade-in\">\r\n    <div class=\"container\">\r\n      <div class=\"brand\">\r\n        <router-link to=\"/\">\r\n          <!-- Pastikan Anda sudah menempatkan logo Anda di src/assets/images/logo.jpg -->\r\n          <img src=\"@/assets/images/logo.png\" alt=\"Ibu Pintar Logo\" class=\"logo-image\">\r\n        </router-link>\r\n      </div>\r\n      <nav class=\"nav-links\">\r\n        <router-link to=\"/\">Beranda</router-link>\r\n        <router-link to=\"/articles\">Artikel</router-link>\r\n        <router-link to=\"/layanan\">Layanan</router-link>\r\n        <router-link to=\"/tanya-bidan\">Tanya Bidan</router-link>\r\n        <router-link to=\"/about\">Tentang Kami</router-link>\r\n      </nav>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  name: 'TheNavbar',\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* ANIMASI MASUK */\r\n@keyframes fadeInDown {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(-20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n.animate-fade-in {\r\n  animation: fadeInDown 0.6s ease-out forwards;\r\n}\r\n\r\n/* STRUKTUR NAVBAR */\r\n.navbar {\r\n  background-color: #ffffff;\r\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.05);\r\n  padding: 15px 0;\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 1000;\r\n  width: 100%;\r\n  transition: all 0.3s ease-in-out;\r\n}\r\n\r\n.container {\r\n  max-width: 1200px;\r\n  margin: 0 auto;\r\n  padding: 0 20px;\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n}\r\n\r\n.brand a {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.logo-image {\r\n  height: 100px; /* Sesuaikan tinggi logo Anda */\r\n  width: auto;\r\n}\r\n\r\n.nav-links {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 15px; /* Memberi jarak antar link */\r\n}\r\n\r\n.nav-links a {\r\n  margin-left: 20px;\r\n  font-weight: 600;\r\n  position: relative;\r\n  text-decoration: none;\r\n  color: #444;\r\n  padding-bottom: 5px;\r\n  transition: color 0.3s ease;\r\n}\r\n\r\n/* Efek Hover dengan garis bawah (underline) */\r\n.nav-links a::after {\r\n  content: '';\r\n  position: absolute;\r\n  left: 0;\r\n  bottom: 0;\r\n  height: 2px;\r\n  width: 0;\r\n  background-color: #c2185b; /* Warna pink tua */\r\n  transition: width 0.3s ease-in-out;\r\n}\r\n\r\n.nav-links a:hover {\r\n  color: #c2185b;\r\n}\r\n\r\n.nav-links a:hover::after {\r\n  width: 100%;\r\n}\r\n\r\n/* Gaya untuk link yang sedang aktif */\r\n.router-link-exact-active {\r\n  color: #c2185b;\r\n  font-weight: 700;\r\n}\r\n\r\n.router-link-exact-active::after {\r\n  width: 100%; /* Garis bawah langsung muncul jika link aktif */\r\n  background-color: #c2185b;\r\n}\r\n</style>\r\n"],"mappings":"AAqBA,eAAe;EACbA,IAAI,EAAE;AACR,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}