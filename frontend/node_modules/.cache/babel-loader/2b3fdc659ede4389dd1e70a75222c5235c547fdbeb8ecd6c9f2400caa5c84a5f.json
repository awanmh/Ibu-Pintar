{"ast":null,"code":"import { ref, onMounted, onUnmounted, computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nimport AppButton from '@/components/common/AppButton.vue';\n\n// --- LOGIKA OTENTIKASI ---\n\nexport default {\n  __name: 'TheNavbar',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const router = useRouter();\n    const isLoggedIn = computed(() => store.getters['auth/isLoggedIn']);\n    const user = computed(() => store.getters['auth/currentUser']);\n    const handleLogout = () => {\n      store.dispatch('auth/logout');\n      router.push('/login'); // Arahkan ke halaman login setelah logout\n    };\n\n    // --- LOGIKA SCROLL NAVBAR ---\n    const isHidden = ref(false);\n    let lastScrollY = window.scrollY;\n    const handleScroll = () => {\n      const currentScroll = window.scrollY;\n      isHidden.value = currentScroll > lastScrollY && currentScroll > 80;\n      lastScrollY = currentScroll <= 0 ? 0 : currentScroll;\n    };\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll);\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll);\n    });\n    const __returned__ = {\n      store,\n      router,\n      isLoggedIn,\n      user,\n      handleLogout,\n      isHidden,\n      get lastScrollY() {\n        return lastScrollY;\n      },\n      set lastScrollY(v) {\n        lastScrollY = v;\n      },\n      handleScroll,\n      ref,\n      onMounted,\n      onUnmounted,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      AppButton\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","onUnmounted","computed","useStore","useRouter","AppButton","store","router","isLoggedIn","getters","user","handleLogout","dispatch","push","isHidden","lastScrollY","window","scrollY","handleScroll","currentScroll","value","addEventListener","removeEventListener"],"sources":["E:/ibuPintar/frontend/src/components/layout/TheNavbar.vue"],"sourcesContent":["<template>\r\n  <header \r\n    class=\"navbar animate-fade-in\" \r\n    :class=\"{ 'navbar-hidden': isHidden }\"\r\n  >\r\n    <div class=\"container\">\r\n      <div class=\"brand\">\r\n        <router-link to=\"/\" class=\"brand-link\">\r\n          <!-- Pastikan path logo Anda benar -->\r\n          <img src=\"@/assets/images/logo2.jpg\" alt=\"Ibu Pintar Logo\" class=\"logo-image\" />\r\n        </router-link>\r\n      </div>\r\n      \r\n      <nav class=\"nav-links\">\r\n        <router-link to=\"/\">Beranda</router-link>\r\n        <router-link to=\"/articles\">Artikel</router-link>\r\n        <router-link to=\"/layanan\">Layanan</router-link>\r\n        <router-link to=\"/tanya-bidan\">Tanya Bidan</router-link>\r\n        <router-link to=\"/about\">Tentang Kami</router-link>\r\n      </nav>\r\n\r\n      <!-- ### BAGIAN INI DIPERBARUI ### -->\r\n      <div class=\"navbar-actions\">\r\n        <!-- Tampilan untuk pengguna yang sudah login -->\r\n        <div v-if=\"isLoggedIn\" class=\"user-menu\">\r\n          <span class=\"user-greeting\">Halo, {{ user.name }}</span>\r\n          <button @click=\"handleLogout\" class=\"logout-btn\">Logout</button>\r\n        </div>\r\n\r\n        <!-- Tampilan untuk tamu (belum login) -->\r\n        <div v-else class=\"guest-menu\">\r\n          <router-link to=\"/login\">\r\n            <AppButton variant=\"secondary\">Masuk</AppButton>\r\n          </router-link>\r\n          <router-link to=\"/register\">\r\n            <AppButton variant=\"primary\">Daftar</AppButton>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted, onUnmounted, computed } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport { useRouter } from 'vue-router';\r\nimport AppButton from '@/components/common/AppButton.vue';\r\n\r\n// --- LOGIKA OTENTIKASI ---\r\nconst store = useStore();\r\nconst router = useRouter();\r\n\r\nconst isLoggedIn = computed(() => store.getters['auth/isLoggedIn']);\r\nconst user = computed(() => store.getters['auth/currentUser']);\r\n\r\nconst handleLogout = () => {\r\n  store.dispatch('auth/logout');\r\n  router.push('/login'); // Arahkan ke halaman login setelah logout\r\n};\r\n\r\n// --- LOGIKA SCROLL NAVBAR ---\r\nconst isHidden = ref(false);\r\nlet lastScrollY = window.scrollY;\r\n\r\nconst handleScroll = () => {\r\n  const currentScroll = window.scrollY;\r\n  isHidden.value = currentScroll > lastScrollY && currentScroll > 80;\r\n  lastScrollY = currentScroll <= 0 ? 0 : currentScroll;\r\n};\r\n\r\nonMounted(() => {\r\n  window.addEventListener('scroll', handleScroll);\r\n});\r\n\r\nonUnmounted(() => {\r\n  window.removeEventListener('scroll', handleScroll);\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n/* ANIMASI MASUK */\r\n@keyframes fadeInDown {\r\n  from { opacity: 0; transform: translateY(-20px); }\r\n  to { opacity: 1; transform: translateY(0); }\r\n}\r\n.animate-fade-in { animation: fadeInDown 0.6s ease-out forwards; }\r\n\r\n/* NAVBAR UTAMA */\r\n.navbar {\r\n  background-color: rgba(255, 255, 255, 0.95);\r\n  backdrop-filter: blur(10px);\r\n  -webkit-backdrop-filter: blur(10px);\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.04);\r\n  padding: 15px 0;\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 1000;\r\n  transition: transform 0.4s ease-in-out;\r\n}\r\n.navbar-hidden { transform: translateY(-100%); }\r\n\r\n/* CONTAINER dengan GRID LAYOUT */\r\n.container {\r\n  max-width: 1300px;\r\n  margin: 0 auto;\r\n  padding: 0 24px;\r\n  display: grid;\r\n  grid-template-columns: 1fr auto 1fr;\r\n  align-items: center;\r\n}\r\n\r\n/* BRAND (LOGO) di Kiri */\r\n.brand { justify-self: start; }\r\n.brand-link { display: flex; }\r\n.logo-image { height: 50px; }\r\n\r\n/* NAV LINKS di Tengah */\r\n.nav-links { justify-self: center; display: flex; align-items: center; gap: 35px; }\r\n.nav-links a { font-weight: 600; position: relative; padding: 6px 0; color: #444; text-decoration: none; transition: all 0.3s ease; }\r\n\r\n/* ACTIONS (Tombol Login/Logout) di Kanan */\r\n.navbar-actions {\r\n  justify-self: end;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n.user-menu, .guest-menu {\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 15px;\r\n}\r\n.user-greeting {\r\n  font-weight: 600;\r\n  color: #333;\r\n}\r\n.logout-btn {\r\n  background: none;\r\n  border: 1px solid #ddd;\r\n  padding: 8px 15px;\r\n  border-radius: 8px;\r\n  font-weight: 600;\r\n  cursor: pointer;\r\n  transition: all 0.2s ease;\r\n}\r\n.logout-btn:hover {\r\n  background-color: #f1f1f1;\r\n  border-color: #ccc;\r\n}\r\n\r\n/* Efek Hover & Active Link */\r\n.nav-links a::after { content: ''; position: absolute; left: 50%; transform: translateX(-50%); bottom: 0; height: 2px; width: 0; background-color: #c2185b; transition: width 0.3s ease-in-out; }\r\n.nav-links a:hover { color: #c2185b; }\r\n.nav-links a:hover::after { width: 100%; }\r\n.router-link-exact-active { color: #c2185b; font-weight: 700; }\r\n.router-link-exact-active::after { width: 100%; }\r\n</style>\r\n"],"mappings":"AA4CA,SAASA,GAAG,EAAEC,SAAS,EAAEC,WAAW,EAAEC,QAAQ,QAAQ,KAAK;AAC3D,SAASC,QAAQ,QAAQ,MAAM;AAC/B,SAASC,SAAS,QAAQ,YAAY;AACtC,OAAOC,SAAS,MAAM,mCAAmC;;AAEzD;;;;;;;;IACA,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,MAAM,GAAGH,SAAS,CAAC,CAAC;IAE1B,MAAMI,UAAU,GAAGN,QAAQ,CAAC,MAAMI,KAAK,CAACG,OAAO,CAAC,iBAAiB,CAAC,CAAC;IACnE,MAAMC,IAAI,GAAGR,QAAQ,CAAC,MAAMI,KAAK,CAACG,OAAO,CAAC,kBAAkB,CAAC,CAAC;IAE9D,MAAME,YAAY,GAAGA,CAAA,KAAM;MACzBL,KAAK,CAACM,QAAQ,CAAC,aAAa,CAAC;MAC7BL,MAAM,CAACM,IAAI,CAAC,QAAQ,CAAC,CAAC,CAAC;IACzB,CAAC;;IAED;IACA,MAAMC,QAAQ,GAAGf,GAAG,CAAC,KAAK,CAAC;IAC3B,IAAIgB,WAAW,GAAGC,MAAM,CAACC,OAAO;IAEhC,MAAMC,YA