nd-color: white;\r\n  border: 1px solid #c2185b;\r\n  color: #c2185b;\r\n  padding: 10px 24px;\r\n  border-radius: 999px;\r\n  cursor: pointer;\r\n  font-weight: 600;\r\n  transition: all 0.3s ease;\r\n}\r\n.filter-btn:hover {\r\n  background-color: #fce4ec;\r\n}\r\n.filter-btn.active {\r\n  background-color: #c2185b;\r\n  color: white;\r\n}\r\n\r\n/* ==== GRID ==== */\r\n.services-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\r\n  gap: 30px;\r\n}\r\n\r\n/* ==== SERVICE CARD ==== */\r\n.service-card {\r\n  background-color: #fff;\r\n  border-radius: 16px;\r\n  overflow: hidden;\r\n  border: 1px solid #e5e7eb;\r\n  box-shadow: 0 4px 14px rgba(0, 0, 0, 0.05);\r\n  transition: transform 0.3s ease, box-shadow 0.3s ease;\r\n}\r\n.service-card:hover {\r\n  transform: translateY(-6px) scale(1.02);\r\n  box-shadow: 0 10px 20px rgba(0,0,0,0.08);\r\n}\r\n\r\n/* ==== IMAGE WRAPPER ==== */\r\n.card-image-wrapper {\r\n  position: relative;\r\n}\r\n.card-image-wrapper img {\r\n  width: 100%;\r\n  height: 250px;\r\n  object-fit: cover;\r\n  background-color: #f9f9f9;\r\n  transition: transform 0.4s ease;\r\n}\r\n.card-image-wrapper img:hover {\r\n  transform: scale(1.03);\r\n}\r\n.category-badge {\r\n  position: absolute;\r\n  top: 15px;\r\n  left: 15px;\r\n  background-color: rgba(255, 255, 255, 0.9);\r\n  padding: 6px 12px;\r\n  border-radius: 8px;\r\n  font-size: 0.75em;\r\n  font-weight: bold;\r\n  color: #c2185b;\r\n  box-shadow: 0 2px 5px rgba(0,0,0,0.1);\r\n}\r\n\r\n/* ==== CARD CONTENT ==== */\r\n.card-content {\r\n  padding: 20px;\r\n}\r\n.card-content h3 {\r\n  margin: 0 0 10px;\r\n  font-size: 1.2em;\r\n  font-weight: 600;\r\n}\r\n.price {\r\n  color: #757575;\r\n  margin-bottom: 15px;\r\n  font-weight: 500;\r\n}\r\n\r\n/* ==== DETAIL BUTTON ==== */\r\n.detail-btn {\r\n  display: block;\r\n  text-align: center;\r\n  background: transparent;\r\n  border: 2px solid #c2185b;\r\n  padding: 10px;\r\n  border-radius: 8px;\r\n  color: #c2185b;\r\n  text-decoration: none;\r\n  font-weight: bold;\r\n  transition: all 0.3s;\r\n}\r\n.detail-btn:hover {\r\n  background-color: #c2185b;\r\n  color: white;\r\n}\r\n</style>\r\n"],"mappings":";;EACOA,KAAK,EAAC;AAA+B;;EAMlCA,KAAK,EAAC;AAAW;;EAChBA,KAAK,EAAC;AAA6B;;EAqBnCA,KAAK,EAAC;AAAe;;EAOjBA,KAAK,EAAC;AAAoB;;;EAEvBA,KAAK,EAAC;AAAgB;;EAEzBA,KAAK,EAAC;AAAc;;EAEpBA,KAAK,EAAC;AAAO;;;uBAzC1BC,mBAAA,CAiDM,OAjDNC,UAiDM,G,0BAhDJC,mBAAA,CAGU;IAHDH,KAAK,EAAC;EAAc,IAC3BG,mBAAA,CAA8C;IAA1CH,KAAK,EAAC;EAAkB,GAAC,cAAY,GACzCG,mBAAA,CAAkG;IAA/FH,KAAK,EAAC;EAAwB,GAAC,8DAA4D,E,sBAGhGG,mBAAA,CA0CO,QA1CPC,UA0CO,GAzCLD,mBAAA,CAmBM,OAnBNE,UAmBM,GAlBJF,mBAAA,CAKS;IAJNG,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,MAAA,CAAAC,SAAS;IAChBV,KAAK,EAAAW,eAAA;MAAAC,MAAA,EAA2BH,MAAA,CAAAI,YAAY;IAAA;KAC9C,mBAED,kBACAV,mBAAA,CAKS;IAJNG,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,MAAA,CAAAC,SAAS;IAChBV,KAAK,EAAAW,eAAA;MAAAC,MAAA,EAA2BH,MAAA,CAAAI,YAAY;IAAA;KAC9C,oBAED,kBACAV,mBAAA,CAKS;IAJNG,OAAK,EAAAC,MAAA,QAAAA,MAAA,MAAAC,MAAA,IAAEC,MAAA,CAAAC,SAAS;IAChBV,KAAK,EAAAW,eAAA;MAAAC,MAAA,EAA2BH,MAAA,CAAAI,YAAY;IAAA;KAC9C,qBAED,iB,GAGFV,mBAAA,CAmBM,OAnBNW,UAmBM,I,kBAlBJb,mBAAA,CAiBMc,SAAA,QAAAC,WAAA,CAhBuBP,MAAA,CAAAQ,gBAAgB,GAAnCC,OAAO,EAAEC,KAAK;yBADxBlB,mBAAA,CAiBM;MAfHmB,GAAG,EAAEF,OAAO,CAACG,EAAE;MAChBrB,KAAK,EAAC,8BAA8B;MACnCsB,KAAK,EAAAC,eAAA;QAAAC,cAAA,KAAuBL,KAAK;MAAA;QAElChB,mBAAA,CAGM,OAHNsB,UAGM,GAFJtB,mBAAA,CAAgD;MAA1CuB,GAAG,EAAER,OAAO,CAACS,KAAK;MAAGC,GAAG,EAAEV,OAAO,CAACW;yCACxC1B,mBAAA,CAA0D,QAA1D2B,UAA0D,EAAAC,gBAAA,CAA1Bb,OAAO,CAACc,QAAQ,iB,GAElD7B,mBAAA,CAMM,OANN8B,UAMM,GALJ9B,mBAAA,CAA2B,YAAA4B,gBAAA,CAApBb,OAAO,CAACW,IAAI,kBACnB1B,mBAAA,CAAuF,KAAvF+B,UAAuF,EAAtE,gBAAc,GAAAH,gBAAA,CAAGb,OAAO,CAACiB,cAAc,CAACC,cAAc,2BACvEC,YAAA,CAEcC,sBAAA;MAFAC,EAAE,cAAcrB,OAAO,CAACsB,IAAI;MAAIxC,KAAK,EAAC;;wBAAa,MAEjE,KAAAO,MAAA,QAAAA,MAAA,O,iBAFiE,0BAEjE,E","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}