{"ast":null,"code":"import { computed, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport ArticleCard from '@/components/articles/ArticleCard.vue';\nexport default {\n  __name: 'ArticlesView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const allArticles = computed(() => store.getters['articles/allArticles']);\n    const status = computed(() => store.state.articles.status);\n    const mainArticle = computed(() => allArticles.value[0] || null);\n    const sideArticles = computed(() => allArticles.value.slice(1, 4));\n    const remainingArticles = computed(() => allArticles.value.slice(4));\n    onMounted(() => {\n      store.dispatch('articles/fetchArticles');\n    });\n    const __returned__ = {\n      store,\n      allArticles,\n      status,\n      mainArticle,\n      sideArticles,\n      remainingArticles,\n      computed,\n      onMounted,\n      get useStore() {\n        return useStore;\n      },\n      ArticleCard\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","onMounted","useStore","ArticleCard","store","allArticles","getters","status","state","articles","mainArticle","value","sideArticles","slice","remainingArticles","dispatch"],"sources":["E:/ibuhamil/frontend/src/views/article/ArticlesView.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-container animate-fade-in\">\r\n    <div v-if=\"status === 'loading'\" class=\"loading\">Memuat artikel...</div>\r\n    <div v-if=\"status === 'error'\" class=\"error\">Gagal memuat artikel. Silakan coba lagi nanti.</div>\r\n\r\n    <div v-if=\"status === 'success' && allArticles.length > 0\">\r\n      <!-- Bagian Atas: Hero & Artikel Pendamping -->\r\n      <div class=\"top-section-grid\">\r\n        <!-- Artikel Utama (Hero) -->\r\n        <div class=\"hero-article animate-slide-up\">\r\n          <ArticleCard :article=\"mainArticle\" :is-hero=\"true\" />\r\n        </div>\r\n        <!-- Artikel Pendamping -->\r\n        <div class=\"side-articles\">\r\n          <ArticleCard\r\n            v-for=\"article in sideArticles\"\r\n            :key=\"article.id\"\r\n            :article=\"article\"\r\n            :is-side=\"true\"\r\n            class=\"animate-slide-up delay\"\r\n          />\r\n        </div>\r\n      </div>\r\n\r\n      <!-- Bagian Bawah: Grid Artikel Lainnya -->\r\n      <div class=\"more-articles-section animate-fade-in\">\r\n        <h2 class=\"section-title\">Artikel Lainnya</h2>\r\n        <div class=\"more-articles-grid\">\r\n          <ArticleCard\r\n            v-for=\"article in remainingArticles\"\r\n            :key=\"article.id\"\r\n            :article=\"article\"\r\n            class=\"animate-fade-in delay\"\r\n          />\r\n        </div>\r\n      </div>\r\n    </div>\r\n\r\n    <div v-else-if=\"status === 'success' && allArticles.length === 0\" class=\"empty-state\">\r\n      <p>Belum ada artikel yang dipublikasikan.</p>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { computed, onMounted } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport ArticleCard from '@/components/articles/ArticleCard.vue';\r\n\r\nconst store = useStore();\r\nconst allArticles = computed(() => store.getters['articles/allArticles']);\r\nconst status = computed(() => store.state.articles.status);\r\n\r\nconst mainArticle = computed(() => allArticles.value[0] || null);\r\nconst sideArticles = computed(() => allArticles.value.slice(1, 4));\r\nconst remainingArticles = computed(() => allArticles.value.slice(4));\r\n\r\nonMounted(() => {\r\n  store.dispatch('articles/fetchArticles');\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n/* Global container */\r\n.page-container {\r\n  max-width: 1200px;\r\n  margin: 40px auto;\r\n  padding: 0 20px;\r\n}\r\n\r\n/* State messages */\r\n.loading, .error, .empty-state {\r\n  text-align: center;\r\n  padding: 50px;\r\n  font-size: 1.2em;\r\n  color: #888;\r\n  animation: fadeIn 0.6s ease-in-out;\r\n}\r\n\r\n/* Grid utama */\r\n.top-section-grid {\r\n  display: grid;\r\n  grid-template-columns: 2fr 1fr;\r\n  gap: 30px;\r\n  margin-bottom: 50px;\r\n}\r\n\r\n/* Artikel pendamping */\r\n.side-articles {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 20px;\r\n}\r\n\r\n/* Bagian bawah */\r\n.more-articles-section {\r\n  margin-top: 40px;\r\n}\r\n\r\n.section-title {\r\n  font-size: 1.8em;\r\n  font-weight: 600;\r\n  border-bottom: 2px solid #f48fb1;\r\n  padding-bottom: 10px;\r\n  margin-bottom: 30px;\r\n  color: #333;\r\n}\r\n\r\n/* Artikel grid */\r\n.more-articles-grid {\r\n  display: grid;\r\n  grid-template-columns: repeat(auto-fill, minmax(280px, 1fr));\r\n  gap: 30px;\r\n}\r\n\r\n/* Animasi modern */\r\n@keyframes fadeIn {\r\n  from { opacity: 0; transform: translateY(10px); }\r\n  to { opacity: 1; transform: translateY(0); }\r\n}\r\n\r\n@keyframes slideUp {\r\n  from { transform: translateY(30px); opacity: 0; }\r\n  to { transform: translateY(0); opacity: 1; }\r\n}\r\n\r\n.animate-fade-in {\r\n  animation: fadeIn 0.8s ease forwards;\r\n}\r\n\r\n.animate-slide-up {\r\n  animation: slideUp 0.9s ease forwards;\r\n}\r\n\r\n.delay {\r\n  animation-delay: 0.2s;\r\n}\r\n\r\n/* Tambahan interaktif */\r\n.ArticleCard:hover {\r\n  transform: translateY(-5px);\r\n  transition: transform 0.3s ease;\r\n}\r\n\r\n</style>\r\n"],"mappings":"AA6CA,SAASA,QAAQ,EAAEC,SAAS,QAAQ,KAAK;AACzC,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,WAAW,MAAM,uCAAuC;;;;;;;IAE/D,MAAMC,KAAK,GAAGF,QAAQ,CAAC,CAAC;IACxB,MAAMG,WAAW,GAAGL,QAAQ,CAAC,MAAMI,KAAK,CAACE,OAAO,CAAC,sBAAsB,CAAC,CAAC;IACzE,MAAMC,MAAM,GAAGP,QAAQ,CAAC,MAAMI,KAAK,CAACI,KAAK,CAACC,QAAQ,CAACF,MAAM,CAAC;IAE1D,MAAMG,WAAW,GAAGV,QAAQ,CAAC,MAAMK,WAAW,CAACM,KAAK,CAAC,CAAC,CAAC,IAAI,IAAI,CAAC;IAChE,MAAMC,YAAY,GAAGZ,QAAQ,CAAC,MAAMK,WAAW,CAACM,KAAK,CAACE,KAAK,CAAC,CAAC,EAAE,CAAC,CAAC,CAAC;IAClE,MAAMC,iBAAiB,GAAGd,QAAQ,CA