{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nimport AppButton from '@/components/common/AppButton.vue';\nexport default {\n  __name: 'LoginView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n\n    // Script-nya sangat mirip dengan RegisterView, hanya memanggil action 'login'\n    const email = ref('');\n    const password = ref('');\n    const error = ref(null);\n    const store = useStore();\n    const router = useRouter();\n    const isLoading = computed(() => store.getters['auth/authStatus'] === 'loading');\n    const handleLogin = async () => {\n      error.value = null;\n      try {\n        await store.dispatch('auth/login', {\n          email: email.value,\n          password: password.value\n        });\n        router.push('/');\n      } catch (err) {\n        error.value = err.response?.data?.message || 'Terjadi kesalahan saat login.';\n      }\n    };\n    const __returned__ = {\n      email,\n      password,\n      error,\n      store,\n      router,\n      isLoading,\n      handleLogin,\n      ref,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      AppButton\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","useStore","useRouter","AppButton","email","password","error","store","router","isLoading","getters","handleLogin","value","dispatch","push","err","response","data","message"],"sources":["E:/ibuPintar/frontend/src/views/auth/LoginView.vue"],"sourcesContent":["<template>\r\n  <div class=\"auth-page\">\r\n    <div class=\"form-container\">\r\n      <h2>Selamat Datang Kembali</h2>\r\n      <form @submit.prevent=\"handleLogin\">\r\n        <div class=\"form-group\">\r\n          <label for=\"email\">