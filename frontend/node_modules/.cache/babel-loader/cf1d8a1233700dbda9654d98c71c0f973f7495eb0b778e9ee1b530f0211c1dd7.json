{"ast":null,"code":"import { ref } from 'vue';\nimport { useRouter } from 'vue-router';\nimport ApiService from '@/services/ApiService';\nimport AppButton from '@/components/common/AppButton.vue';\nexport default {\n  __name: 'AskQuestionView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const router = useRouter();\n    const newQuestion = ref({\n      title: '',\n      content: ''\n    });\n    const notif = ref({\n      message: '',\n      type: '' // 'success' | 'error'\n    });\n    const submitQuestion = async () => {\n      try {\n        await ApiService.createQuestion(newQuestion.value);\n        notif.value.message = '‚úÖ Pertanyaan berhasil dikirim!';\n        notif.value.type = 'success';\n\n        // Reset form\n        newQuestion.value.title = '';\n        newQuestion.value.content = '';\n\n        // Redirect setelah beberapa detik (opsional)\n        setTimeout(() => {\n          router.push('/tanya-bidan');\n        }, 1500);\n      } catch (error) {\n        notif.value.message = '‚ùå Gagal mengirim pertanyaan. Silakan coba lagi.';\n        notif.value.type = 'error';\n      }\n    };\n    const __returned__ = {\n      router,\n      newQuestion,\n      notif,\n      submitQuestion,\n      ref,\n      get useRouter() {\n        return useRouter;\n      },\n      get ApiService() {\n        return ApiService;\n      },\n      AppButton\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","useRouter","ApiService","AppButton","router","newQuestion","title","content","notif","message","type","submitQuestion","createQuestion","value","setTimeout","push","error"],"sources":["E:/ibuPintar/frontend/src/views/QnA/AskQuestionView.vue"],"sourcesContent":["<template>\r\n  <div class=\"page-container form-page animate-fade-in\">\r\n    <h1 class=\"form-title animate-slide-up\">Ajukan Pertanyaan Baru</h1>\r\n    <p class=\"form-sub animate-slide-up delay-1\">\r\n      Tulis pertanyaan Anda dengan jelas dan detail agar mudah dijawab oleh bidan atau member lainnya.\r\n    </p>\r\n\r\n    <form @submit.prevent=\"submitQuestion\" class=\"form-container animate-fade-in delay-2\">\r\n      <div class=\"form-group\">\r\n        <label for=\"title\">Judul Pertanyaan</label>\r\n        <input\r\n          type=\"text\"\r\n          id=\"title\"\r\n          v-model=\"newQuestion.title\"\r\n          required\r\n          placeholder=\"Contoh: Apakah demam setelah vaksin normal?\" />\r\n      </div>\r\n\r\n      <div class=\"form-group\">\r\n        <label for=\"content\">Isi Pertanyaan</label>\r\n        <textarea\r\n          id=\"content\"\r\n          v-model=\"newQuestion.content\"\r\n          rows=\"8\"\r\n          required\r\n          placeholder=\"Tuliskan gejala, usia bayi, dan informasi lainnya secara lengkap...\"></textarea>\r\n      </div>\r\n\r\n      <AppButton type=\"submit\">üí¨ Kirim Pertanyaan</AppButton>\r\n    </form>\r\n\r\n    <!-- NOTIFIKASI -->\r\n    <div v-if=\"notif.message\" :class=\"['notif', notif.type]\">\r\n      {{ notif.message }}\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref } from 'vue';\r\nimport { useRouter } from 'vue-router';\r\nimport ApiService from '@/services/ApiService';\r\nimport AppButton from '@/components/common/AppButton.vue';\r\n\r\nconst router = useRouter();\r\n\r\nconst newQuestion = ref({\r\n  title: '',\r\n  content: '',\r\n});\r\n\r\nconst notif = ref({\r\n  message: '',\r\n  type: '', // 'success' | 'error'\r\n});\r\n\r\nconst submitQuestion = async () => {\r\n  try {\r\n    await ApiService.createQuestion(newQuestion.value);\r\n    notif.value.message = '‚úÖ Pertanyaan berhasil dikirim!';\r\n    notif.value.type = 'success';\r\n\r\n    // Reset form\r\n    newQuestion.value.title = '';\r\n    newQuestion.value.content = '';\r\n\r\n    // Redirect setelah beberapa detik (opsional)\r\n    setTimeout(() => {\r\n      router.push('/tanya-bidan');\r\n    }, 1500);\r\n  } catch (error) {\r\n    notif.value.message = '‚ùå Gagal mengirim pertanyaan. Silakan coba lagi.';\r\n    notif.value.type = 'error';\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* Notifikasi */\r\n.notif {\r\n  margin-top: 20px;\r\n  padding: 15px;\r\n  border-radius: 8px;\r\n  font-weight: 600;\r\n  text-align: center;\r\n}\r\n.success {\r\n  background-color: #e6fffa;\r\n  color: #2f855a;\r\n}\r\n.error {\r\n  background-color: #fff5f5;\r\n  color: #c53030;\r\n}\r\n</style>\r\n"],"mappings":"AAuCA,SAASA,GAAG,QAAQ,KAAK;AACzB,SAASC,SAAS,QAAQ,YAAY;AACtC,OAAOC,UAAU,MAAM,uBAAuB;AAC9C,OAAOC,SAAS,MAAM,mCAAmC;;;;;;;IAEzD,MAAMC,MAAM,GAAGH,SAAS,CAAC,CAAC;IAE1B,MAAMI,WAAW,GAAGL,GAAG,CAAC;MACtBM,KAAK,EAAE,EAAE;MACTC,OAAO,EAAE;IACX,CAAC,CAAC;IAEF,MAAMC,KAAK,GAAGR,GAAG,CAAC;MAChBS,OAAO,EAAE,EAAE;MACXC,IAAI,EAAE,EAAE,CAAE;IACZ,CAAC,CAAC;IAEF,MAAMC,cAAc,GAAG,MAAAA,CAAA,KAAY;MACjC,IAAI;QACF,MAAMT,UAAU,CAACU,cAAc,CAACP,WAAW,CAACQ,KAAK,CAAC;QAClDL,KAAK,CAACK,KAAK,CAACJ,OAAO,GAAG,gCAAgC;QACtDD,KAAK,CAACK,KAAK,CAACH,IAAI,GAAG,SAAS;;QAE5B;QACAL,WAAW,CAACQ,KAAK,CAACP,KAAK,GAAG,EAAE;QAC5BD,WAAW,CAACQ,KAAK,CAACN,OAAO,GAAG,EAAE;;QAE9B;QACAO,UAAU,CAAC,MAAM;UACfV,MAAM,CAACW,IAAI,CAAC,cAAc,CAAC;QAC7B,CAAC,EAAE,IAAI,CAAC;MACV,CAAC,CAAC,OAAOC,KAAK,EAAE;QACdR,KAAK,CAACK,KAAK,CAACJ,OAAO,GAAG,iDAAiD;QACvED,KAAK,CAACK,KAAK,CAACH,IAAI,GAAG,OAAO;MAC5B;IACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}