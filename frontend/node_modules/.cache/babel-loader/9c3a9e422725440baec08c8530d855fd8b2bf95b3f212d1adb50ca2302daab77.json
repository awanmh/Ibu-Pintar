{"ast":null,"code":"import { ref, computed, onMounted, onUnmounted } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nimport AppButton from '@/components/common/AppButton.vue';\nexport default {\n  __name: 'TheNavbar',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const router = useRouter();\n    const isLoggedIn = computed(() => store.getters['auth/isLoggedIn']);\n    const user = computed(() => store.getters['auth/currentUser']);\n    const isAdmin = computed(() => store.getters['auth/isAdmin']);\n    const handleLogout = () => {\n      isDropdownOpen.value = false;\n      sidebarOpen.value = false;\n      store.dispatch('auth/logout');\n      router.push('/login');\n    };\n\n    // --- Scroll logic ---\n    const isHidden = ref(false);\n    let lastScrollY = window.scrollY;\n    const handleScroll = () => {\n      const currentScroll = window.scrollY;\n      isHidden.value = currentScroll > lastScrollY && currentScroll > 80;\n      lastScrollY = currentScroll <= 0 ? 0 : currentScroll;\n    };\n\n    // --- Dropdown user menu (desktop) ---\n    const isDropdownOpen = ref(false);\n    const toggleDropdown = () => {\n      isDropdownOpen.value = !isDropdownOpen.value;\n    };\n    const closeDropdown = event => {\n      if (!event.target.closest('.avatar-btn') && !event.target.closest('.dropdown-menu')) {\n        isDropdownOpen.value = false;\n      }\n    };\n\n    // --- Sidebar (mobile) ---\n    const sidebarOpen = ref(false);\n    const toggleSidebar = () => {\n      sidebarOpen.value = !sidebarOpen.value;\n    };\n    const closeSidebar = () => {\n      sidebarOpen.value = false;\n    };\n    onMounted(() => {\n      window.addEventListener('scroll', handleScroll);\n      document.addEventListener('click', closeDropdown);\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', handleScroll);\n      document.removeEventListener('click', closeDropdown);\n    });\n    const __returned__ = {\n      store,\n      router,\n      isLoggedIn,\n      user,\n      isAdmin,\n      handleLogout,\n      isHidden,\n      get lastScrollY() {\n        return lastScrollY;\n      },\n      set lastScrollY(v) {\n        lastScrollY = v;\n      },\n      handleScroll,\n      isDropdownOpen,\n      toggleDropdown,\n      closeDropdown,\n      sidebarOpen,\n      toggleSidebar,\n      closeSidebar,\n      ref,\n      computed,\n      onMounted,\n      onUnmounted,\n      get useStore() {\n        return useStore;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      AppButton\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","computed","onMounted","onUnmounted","useStore","useRouter","AppButton","store","router","isLoggedIn","getters","user","isAdmin","handleLogout","isDropdownOpen","value","sidebarOpen","dispatch","push","isHidden","lastScrollY","window","scrollY","handleScroll","currentScroll","toggleDropdown","closeDropdown","event","target","closest","toggleSidebar","closeSidebar","addEventListener","document","removeEventListener"],"sources":["E:/ibuPintar/frontend/src/components/layout/TheNavbar.vue"],"sourcesContent":["<template>\r\n  <header class=\"navbar\" :class=\"{ 'navbar-hidden': isHidden }\">\r\n    <div class=\"container\">\r\n      <!-- Hamburger Menu (Mobile) -->\r\n      <button class=\"hamburger\" @click=\"toggleSidebar\">\r\n        &#9776;\r\n      </button>\r\n\r\n      <!-- Logo -->\r\n      <div class=\"brand\">\r\n        <router-link to=\"/\">\r\n          <img src=\"@/assets/images/logo.jpg\" alt=\"Ibu Pintar Logo\" class=\"logo-image\" />\r\n        </router-link>\r\n      </div>\r\n\r\n      <!-- Desktop Navigation -->\r\n      <nav class=\"nav-links desktop-only\">\r\n        <router-link to=\"/\">Beranda</router-link>\r\n        <router-link to=\"/articles\">Artikel</router-link>\r\n        <router-link to=\"/layanan\">Layanan</router-link>\r\n        <router-link to=\"/tanya-bidan\">Tanya Bidan</router-link>\r\n        <router-link to=\"/about\">Tentang Kami</router-link>\r\n      </nav>\r\n\r\n      <!-- Actions -->\r\n      <div class=\"navbar-actions desktop-only\">\r\n        <template v-if=\"isLoggedIn && user\">\r\n          <button @click=\"toggleDropdown\" class=\"avatar-btn\">\r\n            <span>{{ user.name.charAt(0) }}</span>\r\n          </button>\r\n          <transition name=\"dropdown-fade\">\r\n            <div v-if=\"isDropdownOpen\" class=\"dropdown-menu\">\r\n              <div class=\"dropdown-header\">\r\n                <strong>{{ user.name }}</strong>\r\n                <small>{{ user.email }}</small>\r\n              </div>\r\n              <router-link v-if=\"isAdmin\" to=\"/admin\" class=\"dropdown-item\">Dashboard Admin</router-link>\r\n              <a @click=\"handleLogout\" class=\"dropdown-item logout-item\">Logout</a>\r\n            </div>\r\n          </transition>\r\n        </template>\r\n        <template v-else>\r\n          <router-link to=\"/login\"><AppButton variant=\"secondary\">Masuk</AppButton></router-link>\r\n          <router-link to=\"/register\"><AppButton variant=\"primary\">Daftar</AppButton></router-link>\r\n        </template>\r\n      </div>\r\n    </div>\r\n\r\n    <!-- Mobile Sidebar -->\r\n    <transition name=\"slide\">\r\n      <div class=\"mobile-sidebar\" v-if=\"sidebarOpen\">\r\n        <nav class=\"mobile-nav\">\r\n          <router-link to=\"/\" @click=\"closeSidebar\">Beranda</router-link>\r\n          <router-link to=\"/articles\" @click=\"closeSidebar\">Artikel</router-link>\r\n          <router-link to=\"/layanan\" @click=\"closeSidebar\">Layanan</router-link>\r\n          <router-link to=\"/tanya-bidan\" @click=\"closeSidebar\">Tanya Bidan</router-link>\r\n          <router-link to=\"/about\" @click=\"closeSidebar\">Tentang Kami</router-link>\r\n\r\n          <hr />\r\n\r\n          <template v-if=\"isLoggedIn && user\">\r\n            <p><strong>{{ user.name }}</strong></p>\r\n            <router-link v-if=\"isAdmin\" to=\"/admin\" @click=\"closeSidebar\">Dashboard Admin</router-link>\r\n            <a @click=\"handleLogout\">Logout</a>\r\n          </template>\r\n          <template v-else>\r\n            <router-link to=\"/login\" @click=\"closeSidebar\">Masuk</router-link>\r\n            <router-link to=\"/register\" @click=\"closeSidebar\">Daftar</router-link>\r\n          </template>\r\n        </nav>\r\n      </div>\r\n    </transition>\r\n  </header>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, computed, onMounted, onUnmounted } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport { useRouter } from 'vue-router';\r\nimport AppButton from '@/components/common/AppButton.vue';\r\n\r\nconst store = useStore();\r\nconst router = useRouter();\r\n\r\nconst isLoggedIn = computed(() => store.getters['auth/isLoggedIn']);\r\nconst user = computed(() => store.getters['auth/currentUser']);\r\nconst isAdmin = computed(() => store.getters['auth/isAdmin']);\r\n\r\nconst handleLogout = () => {\r\n  isDropdownOpen.value = false;\r\n  sidebarOpen.value = false;\r\n  store.dispatch('auth/logout');\r\n  router.push('/login');\r\n};\r\n\r\n// --- Scroll logic ---\r\nconst isHidden = ref(false);\r\nlet lastScrollY = window.scrollY;\r\nconst handleScroll = () => {\r\n  const currentScroll = window.scrollY;\r\n  isHidden.value = currentScroll > lastScrollY && currentScroll > 80;\r\n  lastScrollY = currentScroll <= 0 ? 0 : currentScroll;\r\n};\r\n\r\n// --- Dropdown user menu (desktop) ---\r\nconst isDropdownOpen = ref(false);\r\nconst toggleDropdown = () => {\r\n  isDropdownOpen.value = !isDropdownOpen.value;\r\n};\r\nconst closeDropdown = (event) => {\r\n  if (!event.target.closest('.avatar-btn') && !event.target.closest('.dropdown-menu')) {\r\n    isDropdownOpen.value = false;\r\n  }\r\n};\r\n\r\n// --- Sidebar (mobile) ---\r\nconst sidebarOpen = ref(false);\r\nconst toggleSidebar = () => {\r\n  sidebarOpen.value = !sidebarOpen.value;\r\n};\r\nconst closeSidebar = () => {\r\n  sidebarOpen.value = false;\r\n};\r\n\r\nonMounted(() => {\r\n  window.addEventListener('scroll', handleScroll);\r\n  document.addEventListener('click', closeDropdown);\r\n});\r\nonUnmounted(() => {\r\n  window.removeEventListener('scroll', handleScroll);\r\n  document.removeEventListener('click', closeDropdown);\r\n});\r\n</script>\r\n\r\n<style scoped>\r\n.navbar {\r\n  background-color: white;\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.04);\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 1000;\r\n  transition: transform 0.4s ease-in-out;\r\n}\r\n.navbar-hidden {\r\n  transform: translateY(-100%);\r\n}\r\n.container {\r\n  display: flex;\r\n  justify-content: space-between;\r\n  align-items: center;\r\n  padding: 12px 20px;\r\n}\r\n.logo-image {\r\n  height: 45px;\r\n}\r\n.nav-links {\r\n  display: flex;\r\n  gap: 24px;\r\n}\r\n.nav-links a {\r\n  text-decoration: none;\r\n  color: #333;\r\n  font-weight: 600;\r\n}\r\n.nav-links a:hover {\r\n  color: #c2185b;\r\n}\r\n.router-link-exact-active {\r\n  color: #c2185b;\r\n}\r\n\r\n/* Hamburger Menu */\r\n.hamburger {\r\n  font-size: 24px;\r\n  background: none;\r\n  border: none;\r\n  cursor: pointer;\r\n  display: block;\r\n}\r\n\r\n/* Avatar */\r\n.avatar-btn {\r\n  width: 40px;\r\n  height: 40px;\r\n  border-radius: 50%;\r\n  background-color: #c2185b;\r\n  color: white;\r\n  font-weight: bold;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  border: none;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Dropdown (desktop) */\r\n.dropdown-menu {\r\n  position: absolute;\r\n  top: 60px;\r\n  right: 20px;\r\n  background: white;\r\n  border-radius: 8px;\r\n  box-shadow: 0 4px 10px rgba(0,0,0,0.1);\r\n  width: 200px;\r\n  padding: 10px;\r\n  z-index: 999;\r\n}\r\n.dropdown-item {\r\n  display: block;\r\n  padding: 10px;\r\n  color: #333;\r\n  text-decoration: none;\r\n}\r\n.dropdown-item:hover {\r\n  background-color: #f5f5f5;\r\n}\r\n.logout-item {\r\n  color: #e74c3c;\r\n  cursor: pointer;\r\n}\r\n\r\n/* Mobile Sidebar */\r\n.mobile-sidebar {\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  height: 100vh;\r\n  width: 260px;\r\n  background-color: white;\r\n  box-shadow: 2px 0 8px rgba(0,0,0,0.15);\r\n  padding: 20px;\r\n  z-index: 2000;\r\n}\r\n.mobile-nav {\r\n  display: flex;\r\n  flex-direction: column;\r\n  gap: 16px;\r\n}\r\n.mobile-nav a {\r\n  text-decoration: none;\r\n  color: #333;\r\n  font-weight: 600;\r\n}\r\n\r\n/* Animation */\r\n.slide-enter-active, .slide-leave-active {\r\n  transition: transform 0.3s ease;\r\n}\r\n.slide-enter-from {\r\n  transform: translateX(-100%);\r\n}\r\n.slide-leave-to {\r\n  transform: translateX(-100%);\r\n}\r\n\r\n/* Responsive */\r\n.desktop-only {\r\n  display: none;\r\n}\r\n@media (min-width: 768px) {\r\n  .hamburger {\r\n    display: none;\r\n  }\r\n  .desktop-only {\r\n    display: flex;\r\n  }\r\n  .mobile-sidebar {\r\n    display: none !important;\r\n  }\r\n}\r\n</style>\r\n"],"mappings":"AA4EA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,SAAS,EAAEC,WAAW,QAAQ,KAAK;AAC3D,SAASC,QAAQ,QAAQ,MAAM;AAC/B,SAASC,SAAS,QAAQ,YAAY;AACtC,OAAOC,SAAS,MAAM,mCAAmC;;;;;;;IAEzD,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,MAAM,GAAGH,SAAS,CAAC,CAAC;IAE1B,MAAMI,UAAU,GAAGR,QAAQ,CAAC,MAAMM,KAAK,CAACG,OAAO,CAAC,iBAAiB,CAAC,CAAC;IACnE,MAAMC,IAAI,GAAGV,QAAQ,CAAC,MAAMM,KAAK,CAACG,OAAO,CAAC,kBAAkB,CAAC,CAAC;IAC9D,MAAME,OAAO,GAAGX,QAAQ,CAAC,MAAMM,KAAK,CAACG,OAAO,CAAC,cAAc,CAAC,CAAC;IAE7D,MAAMG,YAAY,GAAGA,CAAA,KAAM;MACzBC,cAAc,CAACC,KAAK,GAAG,KAAK;MAC5BC,WAAW,CAACD,KAAK,GAAG,KAAK;MACzBR,KAAK,CAACU,QAAQ,CAAC,aAAa,CAAC;MAC7BT,MAAM,CAACU,IAAI,CAAC,QAAQ,CAAC;IACvB,CAAC;;IAED;IACA,MAAMC,QAAQ,GAAGnB,GAAG,CAAC,KAAK,CAAC;IAC3B,IAAIoB,WAAW,GAAGC,MAAM,CAACC,OAAO;IAChC,MAAMC,YAAY,GAAGA,CAAA,KAAM;MACzB,MAAMC,aAAa,GAAGH,MAAM,CAACC,OAAO;MACpCH,QAAQ,CAACJ,KAAK,GAAGS,a