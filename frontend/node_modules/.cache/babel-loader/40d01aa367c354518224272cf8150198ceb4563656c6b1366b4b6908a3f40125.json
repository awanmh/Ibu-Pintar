{"ast":null,"code":"import { ref, onMounted, computed } from 'vue';\nimport { useRoute } from 'vue-router';\nimport ApiService from '@/services/ApiService';\nimport { formatDate } from '@/utils/formatDate';\nexport default {\n  __name: 'ArticleDetailView',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const route = useRoute();\n    const article = ref(null);\n    const loading = ref(true);\n    const error = ref(null);\n    const formattedDate = computed(() => article.value ? formatDate(article.value.date_posted) : '');\n    const articleParagraphs = computed(() => article.value ? article.value.content.split('\\n') : []);\n    onMounted(async () => {\n      const articleId = route.params.id;\n      try {\n        const response = await ApiService.getArticle(articleId);\n        article.value = response.data;\n      } catch (err) {\n        error.value = \"Artikel tidak ditemukan atau terjadi kesalahan.\";\n        console.error(err);\n      } finally {\n        loading.value = false;\n      }\n    });\n    const __returned__ = {\n      route,\n      article,\n      loading,\n      error,\n      formattedDate,\n      articleParagraphs,\n      ref,\n      onMounted,\n      computed,\n      get useRoute() {\n        return useRoute;\n      },\n      get ApiService() {\n        return ApiService;\n      },\n      get formatDate() {\n        return formatDate;\n      }\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["ref","onMounted","computed","useRoute","ApiService","formatDate","route","article","loading","error","formattedDate","value","date_posted","articleParagraphs","content","split","articleId","params","id","response","getArticle","data","err","console"],"sources":["E:/ibuhamil/frontend/src/views/ArticleDetailView.vue"],"sourcesContent":["  <template>\r\n    <div class=\"page-container\">\r\n      <div v-if=\"loading\">Memuat artikel...</div>\r\n      <div v-if=\"error\" class=\"error\">{{ error }}</div>\r\n      <article v-if=\"article\">\r\n        <h1 class=\"article-title\">{{ article.title }}</h1>\r\n        <p class=\"article-meta\">\r\n          Oleh <strong>{{ article.author.name }}</strong> dalam kategori <strong>{{ article.category.name }}</strong>\r\n          <br>\r\n          Dipublikasikan pada {{ formattedDate }}\r\n        </p>\r\n        <div class=\"article-content\">\r\n          <p v-for=\"(paragraph, index) in articleParagraphs\" :key=\"index\">{{ paragraph }}</p>\r\n        </div>\r\n        <router-link to=\"/articles\" class=\"back-link\">‚Üê Kembali ke Semua Artikel</router-link>\r\n      </article>\r\n    </div>\r\n  </template>\r\n\r\n  <script setup>\r\n  import { ref, onMounted, computed } from 'vue';\r\n  import { useRoute } from 'vue-router';\r\n  import ApiService from '@/services/ApiService';\r\n  import { formatDate } from '@/utils/formatDate';\r\n\r\n  const route = useRoute();\r\n  const article = ref(null);\r\n  const loading = ref(true);\r\n  const error = ref(null);\r\n\r\n  const formattedDate = computed(() => article.value ? formatDate(article.value.date_posted) : '');\r\n  const articleParagraphs = computed(() => article.value ? article.value.content.split('\\n') : []);\r\n\r\n  onMounted(async () => {\r\n    const articleId = route.params.id;\r\n    try {\r\n      const response = await ApiService.getArticle(articleId);\r\n      article.value = response.data;\r\n    } catch (err) {\r\n      error.value = \"Artikel tidak ditemukan atau terjadi kesalahan.\";\r\n      console.error(err);\r\n    } finally {\r\n      loading.value = false;\r\n    }\r\n  });\r\n  </script>\r\n\r\n  <style scoped>\r\n  .page-container {\r\n    max-width: 800px;\r\n    margin: 40px auto;\r\n    padding: 0 20px;\r\n  }\r\n  .article-title {\r\n    font-size: 2.5em;\r\n    margin-bottom: 10px;\r\n  }\r\n  .article-meta {\r\n    color: #666;\r\n    font-style: italic;\r\n    margin-bottom: 30px;\r\n    border-bottom: 1px solid #eee;\r\n    padding-bottom: 20px;\r\n  }\r\n  .article-content {\r\n    line-height: 1.8;\r\n    font-size: 1.1em;\r\n  }\r\n  .back-link {\r\n    display: inline-block;\r\n    margin-top: 40px;\r\n    text-decoration: none;\r\n    font-weight: bold;\r\n    color: #ff69b4;\r\n  }\r\n  </style>"],"mappings":"AAoBE,SAASA,GAAG,EAAEC,SAAS,EAAEC,QAAQ,QAAQ,KAAK;AAC9C,SAASC,QAAQ,QAAQ,YAAY;AACrC,OAAOC,UAAU,MAAM,uBAAuB;AAC9C,SAASC,UAAU,QAAQ,oBAAoB;;;;;;;IAE/C,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,OAAO,GAAGP,GAAG,CAAC,IAAI,CAAC;IACzB,MAAMQ,OAAO,GAAGR,GAAG,CAAC,IAAI,CAAC;IACzB,MAAMS,KAAK,GAAGT,GAAG,CAAC,IAAI,CAAC;IAEvB,MAAMU,aAAa,GAAGR,QAAQ,CAAC,MAAMK,OAAO,CAACI,KAAK,GAAGN,UAAU,CAACE,OAAO,CAACI,KAAK,CAACC,WAAW,CAAC,GAAG,EAAE,CAAC;IAChG,MAAMC,iBAAiB,GAAGX,QAAQ,CAAC,MAAMK,OAAO,CAACI,KAAK,GAAGJ,OAAO,CAACI,KAAK,CAACG,OAAO,CAACC,KAAK,CAAC,IAAI,CAAC,GAAG,EAAE,CAAC;IAEhGd,SAAS,CAAC,YAAY;MACpB,MAAMe,SAAS,GAAGV,KAAK,CAACW,MAAM,CAACC,EAAE;MACjC,IAAI;QACF,MAAMC,QAAQ,GAAG,MAAMf,UAAU,CAACgB,UAAU,CAACJ,SAAS,CAAC;QACvDT,OAAO,CAACI,KAAK,GAAGQ,QAAQ,CAACE,IAAI;MAC/B,CAAC,CAAC,OAAOC,GAAG,EAAE;QACZb,KAAK,CAACE,KAAK,GAAG,iDAAiD;QAC/DY,OAAO,CAACd,KAAK,CAACa,GAAG,CAAC;MACpB,CAAC,SAAS;QACRd,OAAO,CAACG,KAAK,GAAG,KAAK;MACvB;IACF,CAAC,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}