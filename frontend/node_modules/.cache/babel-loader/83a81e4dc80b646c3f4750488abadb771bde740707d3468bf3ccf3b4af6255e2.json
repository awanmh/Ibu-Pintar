sContent":["<template>\r\n  <div class=\"max-w-4xl mx-auto p-6\">\r\n    <h2 class=\"text-2xl font-bold mb-6 text-gray-800\">\r\n      {{ isEdit ? 'Edit Treatment' : 'Tambah Treatment' }}\r\n    </h2>\r\n\r\n    <form @submit.prevent=\"handleSubmit\" class=\"bg-white rounded-lg shadow-md p-6 grid grid-cols-1 md:grid-cols-2 gap-4\">\r\n      <div>\r\n        <label class=\"block text-sm font-medium text-gray-700 mb-1\">Nama Treatment</label>\r\n        <input v-model=\"form.title\" type=\"text\" placeholder=\"Nama Treatment\" required class=\"input\" />\r\n      </div>\r\n\r\n      <div>\r\n        <label class=\"block text-sm font-medium text-gray-700 mb-1\">URL Gambar</label>\r\n        <input v-model=\"form.image\" type=\"text\" placeholder=\"https://...\" class=\"input\" />\r\n      </div>\r\n\r\n      <div>\r\n        <label class=\"block text-sm font-medium text-gray-700 mb-1\">Durasi</label>\r\n        <input v-model=\"form.duration\" type=\"text\" placeholder=\"Contoh: 60 menit\" class=\"input\" />\r\n      </div>\r\n\r\n      <div>\r\n        <label class=\"block text-sm font-medium text-gray-700 mb-1\">Kategori</label>\r\n        <input v-model=\"form.category\" type=\"text\" placeholder=\"Contoh: Yoga\" class=\"input\" />\r\n      </div>\r\n\r\n      <div class=\"md:col-span-2\">\r\n        <label class=\"block text-sm font-medium text-gray-700 mb-1\">Deskripsi</label>\r\n        <textarea v-model=\"form.description\" rows=\"3\" placeholder=\"Deskripsi singkat...\" class=\"input resize-none\"></textarea>\r\n      </div>\r\n\r\n      <div>\r\n        <label class=\"block text-sm font-medium text-gray-700 mb-1\">Harga (Rp)</label>\r\n        <input v-model.number=\"form.price\" type=\"number\" min=\"0\" placeholder=\"150000\" class=\"input\" />\r\n      </div>\r\n\r\n      <div class=\"md:col-span-2 flex justify-end mt-4\">\r\n        <button type=\"submit\" class=\"bg-pink-600 hover:bg-pink-700 text-white px-6 py-2 rounded-md shadow\">\r\n          Simpan\r\n        </button>\r\n      </div>\r\n    </form>\r\n  </div>\r\n</template>\r\n\r\n<script setup>\r\nimport { ref, onMounted } from 'vue';\r\nimport { useRoute, useRouter } from 'vue-router';\r\nimport ApiService from '@/services/ApiService';\r\n\r\nconst route = useRoute();\r\nconst router = useRouter();\r\n\r\nconst isEdit = route.name === 'EditTreatment';\r\nconst form = ref({\r\n  title: '',\r\n  image: '',\r\n  duration: '',\r\n  category: '',\r\n  description: '',\r\n  price: 0\r\n});\r\n\r\nonMounted(async () => {\r\n  if (isEdit) {\r\n    const { data } = await ApiService.getTreatmentById(route.params.id);\r\n    form.value = data;\r\n  }\r\n});\r\n\r\nconst handleSubmit = async () => {\r\n  try {\r\n    if (isEdit) {\r\n      await ApiService.updateTreatment(route.params.id, form.value);\r\n    } else {\r\n      await ApiService.createTreatment(form.value);\r\n    }\r\n    router.push('/admin/treatments');\r\n  } catch (err) {\r\n    alert('Gagal menyimpan data!');\r\n    console.error(err);\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n.input {\r\n  @apply w-full border border-gray-300 rounded-md px-3 py-2 text-sm focus:outline-none focus:ring-2 focus:ring-pink-500 focus:border-pink-500;\r\n}\r\n</style>\r\n"],"mappings":"AA+CA,SAASA,GAAG,EAAEC,SAAS,QAAQ,KAAK;AACpC,SAASC,QAAQ,EAAEC,SAAS,QAAQ,YAAY;AAChD,OAAOC,UAAU,MAAM,uBAAuB;;;;;;;IAE9C,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,MAAM,GAAGH,SAAS,CAAC,CAAC;IAE1B,MAAMI,MAAM,GAAGF,KAAK,CAACG,IAAI,KAAK,eAAe;IAC7C,MAAMC,IAAI,GAAGT,GAAG,CAAC;MACfU,KAAK,EAAE,EAAE;MACTC,KAAK,EAAE,EAAE;MACTC,QAAQ,EAAE,EAAE;MACZC,QAAQ,EAAE,EAAE;MACZC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE;IACT,CAAC,CAAC;IAEFd,SAAS,CAAC,YAAY;MACpB,IAAIM,MAAM,EAAE;QACV,MAAM;UAAES;QAAK,CAAC,GAAG,MAAMZ,UAAU,CAACa,gBAAgB,CAACZ,KAAK,CAACa,MAAM,CAACC,EAAE,CAAC;QACnEV,IAAI,CAACW,KAAK,GAAGJ,IAAI;MACnB;IACF,CAAC,CAAC;IAEF,MAAMK,YAAY,GAAG,MAAAA,CAAA,KAAY;MAC/B,IAAI;QACF,IAAId,MAAM,EAAE;UACV,MAAMH,UAAU,CAACkB,eAAe,CAACjB,KAAK,CAACa,MAAM,CAACC,EAAE,EAAEV,IAAI,CAA