{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRouter } from 'vue-router';\nimport AppButton from '@/components/common/AppButton.vue';\nexport default {\n  __name: 'TheNavbar',\n  setup(__props, {\n    expose: __expose\n  }) {\n    __expose();\n    const store = useStore();\n    const router = useRouter();\n    const isLoggedIn = computed(() => store.getters['auth/isLoggedIn']);\n    const user = computed(() => store.getters['auth/currentUser']);\n    const handleLogout = () => {\n      store.dispatch('auth/logout');\n      router.push('/login');\n    };\n    const __returned__ = {\n      store,\n      router,\n      isLoggedIn,\n      user,\n      handleLogout,\n      computed,\n      get useStore() {\n        return useStore;\n      },\n      get useRouter() {\n        return useRouter;\n      },\n      AppButton\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"names":["computed","useStore","useRouter","AppButton","store","router","isLoggedIn","getters","user","handleLogout","dispatch","push"],"sources":["E:/ibuPintar/frontend/src/components/layout/TheNavbar.vue"],"sourcesContent":["<template>\r\n  <header class=\"navbar ...\">\r\n    <div class=\"container\">\r\n      <!-- ... (bagian brand/logo) ... -->\r\n      \r\n      <nav class=\"nav-links\">\r\n        <router-link to=\"/\">Beranda</router-link>\r\n        <router-link to=\"/articles\">Artikel</router-link>\r\n        <!-- ... (link lain) ... -->\r\n      </nav>\r\n\r\n      <div class=\"navbar-actions\">\r\n        <!-- Tampilan dinamis berdasarkan status login -->\r\n        <div v-if=\"isLoggedIn\" class=\"user-menu\">\r\n          <span>Halo, {{ user.name }}</span>\r\n          <button @click=\"handleLogout\" class=\"logout-btn\">Logout</button>\r\n        </div>\r\n        <div v-else class=\"guest-menu\">\r\n          <router-link to=\"/login\">\r\n            <AppButton variant=\"secondary\">Masuk</AppButton>\r\n          </router-link>\r\n          <router-link to=\"/register\">\r\n            <AppButton variant=\"primary\">Daftar</AppButton>\r\n          </router-link>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </header>\r\n</template>\r\n\r\n<script setup>\r\nimport { computed } from 'vue';\r\nimport { useStore } from 'vuex';\r\nimport { useRouter } from 'vue-router';\r\nimport AppButton from '@/components/common/AppButton.vue';\r\n\r\nconst store = useStore();\r\nconst router = useRouter();\r\n\r\nconst isLoggedIn = computed(() => store.getters['auth/isLoggedIn']);\r\nconst user = computed(() => store.getters['auth/currentUser']);\r\n\r\nconst handleLogout = () => {\r\n  store.dispatch('auth/logout');\r\n  router.push('/login');\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* ANIMASI MASUK */\r\n@keyframes fadeInDown {\r\n  from {\r\n    opacity: 0;\r\n    transform: translateY(-20px);\r\n  }\r\n  to {\r\n    opacity: 1;\r\n    transform: translateY(0);\r\n  }\r\n}\r\n.animate-fade-in {\r\n  animation: fadeInDown 0.6s ease-out forwards;\r\n}\r\n\r\n/* NAVBAR UTAMA */\r\n.navbar {\r\n  background-color: rgba(255, 255, 255, 0.95);\r\n  backdrop-filter: blur(10px);\r\n  -webkit-backdrop-filter: blur(10px);\r\n  box-shadow: 0 2px 12px rgba(0, 0, 0, 0.04);\r\n  padding: 15px 0;\r\n  position: sticky;\r\n  top: 0;\r\n  z-index: 1000;\r\n  transition: transform 0.4s ease-in-out;\r\n}\r\n\r\n/* HIDE navbar saat scroll down */\r\n.navbar-hidden {\r\n  transform: translateY(-100%);\r\n}\r\n\r\n/* CONTAINER dengan GRID LAYOUT */\r\n.container {\r\n  max-width: 1300px;\r\n  margin: 0 auto;\r\n  padding: 0 24px;\r\n  display: grid;\r\n  grid-template-columns: 1fr auto 1fr; /* Kiri | Tengah | Kanan */\r\n  align-items: center;\r\n}\r\n\r\n/* BRAND (LOGO) di Kiri */\r\n.brand {\r\n  justify-self: start; /* Rata kiri */\r\n}\r\n.brand-link {\r\n  display: flex;\r\n}\r\n.logo-image {\r\n  height: 50px;\r\n}\r\n\r\n/* NAV LINKS di Tengah */\r\n.nav-links {\r\n  justify-self: center; /* Rata tengah */\r\n  display: flex;\r\n  align-items: center;\r\n  gap: 35px; /* Jarak antar link */\r\n}\r\n.nav-links a {\r\n  font-weight: 600;\r\n  position: relative;\r\n  padding: 6px 0;\r\n  color: #444;\r\n  text-decoration: none;\r\n  transition: all 0.3s ease;\r\n}\r\n\r\n/* ACTIONS (Placeholder) di Kanan */\r\n.navbar-actions {\r\n  justify-self: end; /* Rata kanan */\r\n  /* Width sama dengan brand agar nav-links benar-benar di tengah */\r\n  width: 162px; /* Sesuaikan dengan lebar logo Anda */\r\n}\r\n\r\n/* Hover & underline effect */\r\n.nav-links a::after {\r\n  content: '';\r\n  position: absolute;\r\n  left: 50%;\r\n  transform: translateX(-50%);\r\n  bottom: 0;\r\n  height: 2px;\r\n  width: 0;\r\n  background-color: #c2185b;\r\n  transition: width 0.3s ease-in-out;\r\n}\r\n.nav-links a:hover {\r\n  color: #c2185b;\r\n}\r\n.nav-links a:hover::after {\r\n  width: 100%;\r\n}\r\n\r\n/* ACTIVE LINK */\r\n.router-link-exact-active {\r\n  color: #c2185b;\r\n  font-weight: 700;\r\n}\r\n.router-link-exact-active::after {\r\n  width: 100%;\r\n}\r\n</style>\r\n"],"mappings":"AA+BA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,SAASC,QAAQ,QAAQ,MAAM;AAC/B,SAASC,SAAS,QAAQ,YAAY;AACtC,OAAOC,SAAS,MAAM,mCAAmC;;;;;;;IAEzD,MAAMC,KAAK,GAAGH,QAAQ,CAAC,CAAC;IACxB,MAAMI,MAAM,GAAGH,SAAS,CAAC,CAAC;IAE1B,MAAMI,UAAU,GAAGN,QAAQ,CAAC,MAAMI,KAAK,CAACG,OAAO,CAAC,iBAAiB,CAAC,CAAC;IACnE,MAAMC,IAAI,GAAGR,QAAQ,CAAC,MAAMI,KAAK,CAACG,OAAO,CAAC,kBAAkB,CAAC,CAAC;IAE9D,MAAME,YAAY,GAAGA,CAAA,KAAM;MACzBL,KAAK,CAACM,QAAQ,CAAC,aAAa,CAAC;MAC7BL,MAAM,CAACM,IAAI,CAAC,QAAQ,CAAC;IACvB,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}